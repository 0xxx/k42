<!DOCTYPE article PUBLIC "-//OASIS//DTD DocBook V3.1//EN" [

<!-- To disable review comments replace INCLUDE with IGNORE -->
<!ENTITY % review "INCLUDE" >

<!-- CVS gives us these -->
<!ENTITY user "$Author: butrico $">
<!ENTITY date "$Date: 2004/01/30 21:07:38 $">
<!ENTITY revision "$Revision">

<!-- define and read in entities from this file -->
<!ENTITY % wp.ent SYSTEM "wp.ent">
%wp.ent;
]>


<!--
    In DocBook terms, a white paper is and article.
-->
<article class="whitepaper" status="draft" lang="en">
  <title>Title of White Paper</title>
  &K42ArtHeader;

  <abstract>
    <para>
      This is the abstract.
    </para>
  </abstract>

  <section id="Template.sect1">
    <title>Section Title Goes Here</title>
    <para>
      Section contents begin with this paragraph.
    </para>
    <para>
      Second paragraph of <xref linkend="Template.sect1">.
    </para>
    <section id="Template.subsection-1.1">
      <title>Subsection Title Goes Here</title>
      <para>
	Subsections content begins with this paragraph.
      </para>
    </section>
  </section>

<!-- biblioentry containing many of the userful features - please add -->
  <bibliography>
    <title>References</title>
    <biblioentry id="myref00" xreflabel="mylabel00">
      <title> </title>
      <authorgroup>
	<author> <firstname> </firstname> 
	  <surname> </surname> </author>
      </authorgroup>
      <confgroup>
	<confdates> </confdates>
	<conftitle> </conftitle>
      </confgroup>
      <seriesvolnums>  </seriesvolnums>
      <volumenum>  </volumenum>
      <pagenums> </pagenums>
      <edition> </edition>
      <date> </date>
      <publisher>
	<publishername> </publishername>
	<address> <street> </street> </address>
      </publisher>
    </biblioentry>
  </bibliography>

</article>

